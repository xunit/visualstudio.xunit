<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>xunit.runner.visualstudio.wp8</id>
    <version>0.99.9</version>
    <title>xUnit.net [Visual Studio Windows Phone 8 Silverlight support]</title>
    <authors>James Newkirk, Brad Wilson</authors>
    <description>Includes support for running xUnit.net v2 tests from Visual Studio 2012+ for Windows Phone 8.1+ applications.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <!-- Discovery -->
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.abstractions.dll"                    target="build\_common\xunit.abstractions.dll" />
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.utility.dll"                  target="build\_common\xunit.runner.utility.dll" />
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.visualstudio.testadapter.dll" target="build\_common\xunit.runner.visualstudio.testadapter.dll" />
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.visualstudio.testadapter.pdb" target="build\_common\xunit.runner.visualstudio.testadapter.pdb" />

    <!-- Execution -->
    <file src="build\_._"                                                                       target="lib\wp8\_._" />
    <file src="build\xunit.runner.visualstudio.props"                                           target="build\wp8\xunit.runner.visualstudio.wp8.props" />
    <file src="build\xunit.runner.visualstudio.wp8.targets"                                     target="build\wp8\xunit.runner.visualstudio.wp8.targets" />
    <file src="xunit.runner.visualstudio.wp8\bin\Release\xunit.runner.utility.dll"              target="build\wp8\device\xunit.runner.utility.dll" />
    <file src="xunit.runner.visualstudio.wp8\bin\Release\xunit.runner.visualstudio.wp8.dll"     target="build\wp8\device\xunit.runner.visualstudio.wp8.dll" />
    <file src="xunit.runner.visualstudio.wp8\bin\Release\xunit.runner.visualstudio.wp8.pdb"     target="build\wp8\device\xunit.runner.visualstudio.wp8.pdb" />
  </files>
</package>