    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <references>
      <reference file="xunit.runner.visualstudio.testadapter.dll" />
    </references>
  </metadata>
  <files>
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.abstractions.dll" target="lib\" />
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.utility.dll" target="lib\" />
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.visualstudio.testadapter.dll" target="lib\" />
    <file src="xunit.runner.visualstudio.testadapter\**\*.cs" target="src" />
  </files>
</package>